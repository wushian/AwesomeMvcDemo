@model AwesomeMvcDemo.ViewModels.Input.LookupDemoCfgInput
<div class="efield">
    <div class="elabel">
        Pick a meal :
    </div>
    <div class="einput">
    @*begin*@
    @(Html.Awe().LookupFor(o => o.Meal)
          .ClearButton(Model.ClearButton)
          .HighlightChange(Model.HighlightChange)
          .Height(Model.Height)
          .Width(Model.Width)
          .Fullscreen(Model.Fullscreen))
        @*end*@
    </div>
</div>
