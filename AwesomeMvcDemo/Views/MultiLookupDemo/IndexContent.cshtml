@model AwesomeMvcDemo.ViewModels.Input.MultiLookupDemoCfgInput
<div class="efield">
    <div class="elabel">
        Pick some Meals:</div>
    <div class="einput">
        @*begin*@
@(Html.Awe().MultiLookupFor(o => o.Meals)
        .ClearButton(Model.ClearButton)
        .HighlightChange(Model.HighlightChange)
        .DragAndDrop(Model.DragAndDrop)
        .Height(Model.Height)
        .Width(Model.Width)
        .Fullscreen(Model.Fullscreen))
        @*end*@
    </div>
</div>

